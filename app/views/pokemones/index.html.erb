<p style="color: green"><%= notice %></p>

<% content_for :title, "Pokemones" %>

<h1>Pokemones</h1>

<div id="pokemones">
  <% @pokemones.each do |pokemon| %>
    <%= render pokemon %>
    <p>
      <%= link_to "Show this pokemon", pokemon %>
    </p>
  <% end %>
</div>

<%= link_to "New pokemon", new_pokemon_path %>
